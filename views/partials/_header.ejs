<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>

    <% if(username !== undefined){ %>
      
      <h5>Logged in as: <%- username.email %></h5>
      <form method="POST" action="/logout">
        <button type="submit" class=" align-middle btn btn-primary">Logout</button>
        </button>
        
      </form>  
    <% } else{ %>  
      <div class="container">
        <form method="GET" class="row" action="/login" >
          <button type="submit" class=" btn btn-primary">Login</button>
         
        </button>
      </form class="row">  
         <form method="GET" action="/register/">
         <button type="submit" class=" btn btn-primary">Register</button>
         </button>
  
        </form>  
      </div>
   <% } %>
   

  </nav>
</header>

